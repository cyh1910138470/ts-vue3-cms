# https://editorconfig.org

root = true

[*] #表示所有文件适用
charset = utf-8 #设置文件字符集为utf-8
indent_style = space 
indent_size = 2
end_of_line = lf #控制换行类型
insert_final_newline = true #去除首行的任意空白字符
trim_trailing_whitespace = true #始终在文件末尾插入一个新行

[*.md] #表示仅md文件适用以下规则
insert_final_newline = false
trim_trailing_whitespace = false
